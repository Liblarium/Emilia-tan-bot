{
  "name": "@emilia-tan/soma-di",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts || echo 'No files to lint.'",
    "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx}' || echo 'No files to format.'",
    "upd-deps": "ncu -u"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "dependencies": {
    "debug": "^4.4.1",
    "deepmerge-ts": "^7.1.5",
    "emittery": "^1.2.0",
    "json-stable-stringify": "^1.3.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "eslint": "^9.29.0",
    "prettier": "^3.6.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3"
  }
}