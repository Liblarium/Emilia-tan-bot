{
  "name": "@emilia-tan/network",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts || echo 'No files to lint.'",
    "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx}' || echo 'No files to format.'"
  },"exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "dependencies": {
    "axios": "^1.9.0",
    "undici": "^7.9.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "@types/node": "^22.15.17"
  }
}