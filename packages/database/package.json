{
  "name": "@emilia-tan/database",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts || echo 'No files to lint.'",
    "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx}' || echo 'No files to format.'"
  },
  "dependencies": {
    "prisma": "^6.6.0",
    "pg": "^8.15.6",
    "mysql2": "^3.14.0",
    "mongodb": "^6.16.0"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^12.14.0",
    "typescript": "^5.8.3",
    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "@types/node": "^22.15.2"
  },
  "optionalDependencies": {
    "@emilia-tan/core": "workspace:^",
    "@emilia-tan/decorators": "workspace:^",
    "@emilia-tan/media": "workspace:^",
    "@emilia-tan/utlis": "workspace:^",
    "@emilia-tan/logger": "workspace:^",
    "@emilia-tan/discord": "workspace:^",
    "@emilia-tan/types": "workspace:^",
    "@emilia-tan/network": "workspace:^"
  }
}