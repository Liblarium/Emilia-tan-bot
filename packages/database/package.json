{
  "name": "@emilia-tan/database",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts || echo 'No files to lint.'",
    "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx}' || echo 'No files to format.'",
    "upd-deps": "ncu -u"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "dependencies": {
    "mongodb": "^6.17.0",
    "mysql2": "^3.14.1",
    "pg": "^8.16.2",
    "prisma": "^6.10.1"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^12.17.0",
    "@types/node": "^24.0.4",
    "eslint": "^9.29.0",
    "mongoose": "^8.16.0",
    "prettier": "^3.6.0",
    "typescript": "^5.8.3"
  }
}