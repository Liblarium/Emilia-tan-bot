{
  "name": "@emilia-tan/database",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc src -d dist && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts || echo 'No files to lint.'",
    "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx}' || echo 'No files to format.'"
  },"exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "dependencies": {
    "prisma": "^6.6.0",
    "pg": "^8.15.6",
    "mysql2": "^3.14.0",
    "mongodb": "^6.16.0"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^12.14.0",
    "typescript": "^5.8.3",
    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "@types/node": "^22.15.2"
  }
}